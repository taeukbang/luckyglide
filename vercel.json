{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/mrt-partner-link.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/calendar-window-db.ts": {
      "maxDuration": 60
    }
  }
}


